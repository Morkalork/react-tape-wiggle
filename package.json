{
  "name": "react-tape-wiggle",
  "version": "1.0.0",
  "description": "A basic react project using tape for testing",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "test": "npm run lint && tape -r babel-register 'src/**/*-test.js' | tap-spec",
    "browserify": "browserify -d src/index.js -o build/index.js -t [ babelify ]",
    "markup": "cp src/index.html build",
    "css": "node-sass --source-map true src/scss/main.scss build/css/main.css",
    "build": "mkdir -p build && npm run browserify && npm run markup && npm run css"
  },
  "keywords": [
    "react",
    "tape",
    "wiggle"
  ],
  "author": "Magnus Ferm",
  "license": "MIT",
  "dependencies": {
    "@benmosher/redux-promise": "^0.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hyperscript-helpers": "^1.2.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.1",
    "node-sass": "^4.4.0",
    "proxyquire": "^1.7.11",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^1.17.7",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "watch": "^1.0.1"
  }
}
